{
  "name": "photato-website-frontend",
  "version": "3.1.0",
  "description": "This is the JS front end of the Photato website.",
  "main": "src/main.mjs",
  "scripts": {
    "test": "npx jest",
    "postinstall": "npx snowpack --clean",
    "build": "babel src --out-dir public --delete-dir-on-start --keep-file-extension --copy-files",
    "watch": "babel src --out-dir public --delete-dir-on-start --keep-file-extension --copy-files --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vdavid/photato-website-frontend.git"
  },
  "keywords": [
    "photato",
    "photo",
    "photography",
    "website",
    "potato"
  ],
  "author": "David Veszelovszki",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vdavid/photato-website-frontend/issues"
  },
  "homepage": "https://github.com/vdavid/photato-website-frontend#readme",
  "dependencies": {
    "htm": "^3.0.4",
    "react": "npm:@pika/react@^16.13.1",
    "react-dom": "npm:@pika/react-dom@^16.13.1",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/helper-builder-react-jsx": "^7.9.0",
    "jest": "^25.4.0",
    "snowpack": "^1.7.1"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "snowpack": {
    "webDependencies": [
      "react",
      "react-dom",
      "react-router-dom"
    ]
  }
}
